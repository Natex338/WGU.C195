<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="391.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ViewCustomers">
   <children>
      <Button fx:id="addCustButton" layoutX="517.0" layoutY="360.0" mnemonicParsing="false" onAction="#onAddCust" text="Add" />
      <Button fx:id="modifyCustButton" layoutX="558.0" layoutY="360.0" mnemonicParsing="false" onAction="#onModCust" text="Modify" />
      <Button fx:id="deleteCustButton" layoutX="614.0" layoutY="360.0" mnemonicParsing="false" onAction="#onDeleteCust" text="Delete" />
      <Button layoutX="597.0" layoutY="12.0" mnemonicParsing="false" onAction="#onHomePage" text="Homepage" />
      <TableView fx:id="customerListView" layoutX="15.0" layoutY="58.0" prefHeight="299.0" prefWidth="648.0">
        <columns>
          <TableColumn fx:id="custIDCol" prefWidth="37.0" text="ID" />
          <TableColumn fx:id="custNameCol" prefWidth="132.0" text="Customer Name" />
            <TableColumn fx:id="custCountryCol" prefWidth="64.0" text="Country" />
            <TableColumn fx:id="custAddrCol" prefWidth="294.0" text="Customer Address" />
            <TableColumn fx:id="custPhoneCol" prefWidth="120.0" text="Phone" />
        </columns>
      </TableView>
      <Label layoutX="26.0" layoutY="7.0" text="Customer List">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
